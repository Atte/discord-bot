{
    "name": "discord-bot-webui",
    "version": "0.0.1",
    "license": "MIT",
    "private": true,
    "scripts": {
        "build": "yarn run pre-render && parcel build --no-autoinstall ./src/index.html",
        "watch": "parcel watch --no-autoinstall --no-hmr ./src/index.html",
        "rebuild": "rm dist/*; yarn run build",
        "lint": "eslint .",
        "format": "prettier --write .",
        "test": "jest",
        "pre-render": "ts-node --prefer-ts-exts --compiler-options '{\"module\":\"CommonJS\"}' ./pre-render.tsx && prettier --write ./src/index.html ./src/uikit.ts ./src/uikit.less"
    },
    "targets": {
        "default": {
            "publicUrl": "static"
        }
    },
    "packageManager": "yarn@3.0.2",
    "devDependencies": {
        "@parcel/transformer-inline-string": "^2.0.0-rc.0",
        "@parcel/transformer-less": "^2.0.0-rc.0",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/preact": "^2.0.1",
        "@types/glob": "^7.1.4",
        "@types/history": "^4.7.9",
        "@types/jest": "^27.0.2",
        "@types/node": "^16.10.2",
        "@typescript-eslint/eslint-plugin": "^4.32.0",
        "@typescript-eslint/parser": "^4.32.0",
        "eslint": "^7.32.0",
        "eslint-config-preact": "^1.1.4",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-compat": "^3.13.0",
        "eslint-plugin-jest": "^24.5.2",
        "eslint-plugin-react": "^7.26.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "glob": "^7.2.0",
        "jest": "^27.2.4",
        "parcel": "^2.0.0-rc.0",
        "preact-render-to-string": "^5.1.19",
        "prettier": "2.4.1",
        "ts-jest": "^27.0.5",
        "ts-node": "^10.2.1",
        "typescript": "^4.4.3"
    },
    "dependencies": {
        "history": "^4.10.1",
        "preact": "^10.5.14",
        "preact-router": "^3.2.1",
        "uikit": "^3.7.4"
    }
}
